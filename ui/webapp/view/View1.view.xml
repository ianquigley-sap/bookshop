<mvc:View controllerName="com.sap.app.ui5.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>  
                    <List noDataText="Drop list items here" id="list0" items="{/Books}">
                        <items>
                            <StandardListItem type="Navigation" title="{title}" description="{stock}" icon="sap-icon://picture" id="item0"/>
                        </items>
                    </List>
                </content>
			</Page>
		</pages>
	</App>
</mvc:View>